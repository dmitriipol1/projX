<div class="container-fluid">
  <div class="container">
    <div>
      <div style="padding: 10px;" class="row">
        <div class="col-xs-4">
          <div class="cat">
            <div class="list-group">
              <% @cats.each do |theme| %>
                  <a href="<%= theme['category_id'] %>" class="list-group-item list-group-item-action">
                    <%= theme['category_text'] %></a>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
